#include <cstdlib>
#include <stdlib.h>
#include <unistd.h>

using namespace std;

/*
 * 
 */
int main(int argc, char** argv) {
    system("echo 1 > /sys/class/gpio/gpio4/value");
    sleep(3);
    system("echo 0 > /sys/class/gpio/gpio4/value");
    return 0;
}

